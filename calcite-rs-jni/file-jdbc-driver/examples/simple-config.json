{
  "version": "1.0",
  "name": "Simple Analytics", 
  "description": "Basic analytics data lake",
  "defaultSchema": "analytics",
  
  "executionEngine": "parquet",
  "batchSize": 2048,
  "memoryThreshold": 268435456,
  "refreshInterval": "1 hour",
  
  "awsRegion": "us-east-1",
  
  "schemas": [
    {
      "name": "analytics",
      "operand": {
        "directory": "/data/analytics",
        "recursive": true
      }
    }
  ]
}